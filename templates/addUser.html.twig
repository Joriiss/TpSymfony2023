{%  extends "base.html.twig" %}

{%  block body %}
    <h1>Register a new user</h1>

    {{ form_start(form) }}
        {{ form_row(form.email) }}
        {{ form_row(form.password) }}
        {{ form_row(form.roles) }}
    <button type="submit">Register</button>
    {{ form_end(form) }}
{%  endblock %}